{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "indent": 0,
    "no-tabs": 0,
    "padded-blocks": 0,
    "no-console": 0,
    "no-trailing-spaces": 0,
    "eol-last": 0,
    "prefer-destructuring": 0,
    "function-paren-newline": 0,
    "object-shorthand": 0,
    "comma-dangle": 0
  }
}